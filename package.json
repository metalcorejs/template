    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "app-root-path": "^3.0.0",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "chalk": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.1.2",
    "esm": "^3.2.25",
